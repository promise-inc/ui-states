{
  "name": "@promisejs/ui-states",
  "version": "0.1.0",
  "description": "Auto-generated skeleton loading states from real DOM â€” zero config React component",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "type-check": "tsc --noEmit",
    "dev": "tsup --watch"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0"
  },
  "keywords": [
    "react",
    "skeleton",
    "loading",
    "ui-states",
    "auto-skeleton",
    "loading-state"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/promise-inc/ui-states"
  },
  "sideEffects": false
}
